{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["out/src", "out/src/api" ]
    },
    "tags": {
        "dictionaries": ["jsdoc","closure"],
        "allowUnknownTags": true
    },
    "templates": {
        "monospaceLinks": false,
        "cleverLinks": false,
        "default": {
            "staticFiles": {
                "include": [
                    "./jsdoc-template/static"
                ]
	          },
	          "outputSourceFiles": false,
                "showTutorials": false
            }
      },
      "opts": {
          "destination": "./docs/",
          "private": false
      }
}
